<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="EditTextINRMeterTheme" parent="android:Widget.EditText">
        <item name="android:background">@drawable/inrmetertheme_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="AutoCompleteTextViewINRMeterTheme" parent="android:Widget.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/inrmetertheme_list_selector_holo_light</item>
        <item name="android:background">@drawable/inrmetertheme_edit_text_holo_light</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="CheckBoxINRMeterTheme" parent="android:Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/inrmetertheme_btn_check_holo_light</item>
    </style>

    <style name="RadioButtonINRMeterTheme" parent="android:Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/inrmetertheme_btn_radio_holo_light</item>
    </style>

    <style name="ButtonINRMeterTheme" parent="android:Widget.Button">
        <item name="android:background">@drawable/inrmetertheme_btn_default_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:minWidth">64dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ImageButtonINRMeterTheme" parent="android:Widget.ImageButton">
        <item name="android:background">@drawable/inrmetertheme_btn_default_holo_light</item>
    </style>

    <style name="SpinnerINRMeterTheme" parent="android:Widget.Spinner">
        <item name="android:background">@drawable/inrmetertheme_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/inrmetertheme_list_selector_holo_light</item>
    </style>

    <style name="SpinnerDropDownItemINRMeterTheme" parent="android:Widget.DropDownItem.Spinner">
        <item name="android:checkMark">@drawable/inrmetertheme_btn_radio_holo_light</item>
    </style>

    <style name="TabINRMeterTheme">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/inrmetertheme_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextINRMeterTheme">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ProgressBarINRMeterTheme" parent="android:Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/inrmetertheme_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/inrmetertheme_progress_indeterminate_horizontal_holo_light</item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="SeekBarINRMeterTheme" parent="android:Widget.SeekBar">
        <item name="android:progressDrawable">@drawable/inrmetertheme_scrubber_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/inrmetertheme_scrubber_progress_horizontal_holo_light</item>
        <item name="android:minHeight">13dip</item>
        <item name="android:maxHeight">13dip</item>
        <item name="android:thumb">@drawable/inrmetertheme_scrubber_control_selector_holo_light</item>
        <item name="android:thumbOffset">16dip</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
    </style>

    <style name="RatingBarINRMeterTheme" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/inrmetertheme_ratingbar_full_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/inrmetertheme_ratingbar_full_holo_light</item>
    </style>

    <style name="RatingBarBigINRMeterTheme" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/inrmetertheme_ratingbar_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/inrmetertheme_ratingbar_holo_light</item>
        <item name="android:minHeight">35dip</item>
        <item name="android:maxHeight">35dip</item>
    </style>

    <style name="RatingBarSmallINRMeterTheme" parent="android:Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/inrmetertheme_ratingbar_small_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/inrmetertheme_ratingbar_small_holo_light</item>
        <item name="android:minHeight">16dip</item>
        <item name="android:maxHeight">16dip</item>
    </style>

    <style name="ToggleINRMeterTheme" parent="android:Widget.Button.Toggle">
        <item name="android:background">@drawable/inrmetertheme_btn_toggle_holo_light</item>
        <item name="android:minHeight">48dip</item>
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ListViewINRMeterTheme" parent="android:Widget.ListView">
        <item name="android:listSelector">@drawable/inrmetertheme_list_selector_holo_light</item>
    </style>

    <style name="ListViewINRMeterTheme.White" parent="android:Widget.ListView.White">
        <item name="android:listSelector">@drawable/inrmetertheme_list_selector_holo_light</item>
    </style>

    <style name="SpinnerItemINRMeterTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textColor">#000000</item>
    </style>

    <style name="ActionBar.Solid.Example" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_example</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_example</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_example</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Example</item>
    </style>

    <style name="ActionBar.Transparent.Example" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@drawable/ab_transparent_example</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Example</item>
    </style>

    <style name="PopupMenu.Example" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_example</item>
    </style>

    <style name="DropDownListView.Example" parent="@android:style/Widget.Holo.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_example</item>
    </style>

    <style name="ActionBarTabStyle.Example" parent="@android:style/Widget.Holo.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_example</item>
    </style>

    <style name="DropDownNav.Example" parent="@android:style/Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_example</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_example</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_example</item>
    </style>

    <style name="ProgressBar.Example" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_example</item>
    </style>

    <style name="ActionButton.CloseMode.Example" parent="@android:style/Widget.Holo.Light.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_example</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Example.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Example</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Example</item>
    </style>

</resources>